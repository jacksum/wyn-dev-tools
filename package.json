{
	"name": "hoverconverter",
	"displayName": "Hover Converter",
	"description": "Hover over Hex or Decimal values to quickly get their corresponding values in Binary, Hex or Decimal.",
	"version": "0.0.1",
	"publisher": "boolxiao",
	"repository": "https://github.com/bedrankara/hoverconverter",
	"homepage": "https://github.com/bedrankara/hoverconverter",
	"icon": "logo.png",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"keywords": [
		"Converter",
		"Hover",
		"Hex",
		"Decimal",
		"Hover Converter"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.enableHoverconverter",
				"title": "Hover Converter: Enable Extension"
			},
			{
				"command": "extension.disableHoverconverter",
				"title": "Hover Converter: Disable Extension "
			},
			{
				"command": "extension.deleteAllBranch",
				"title": "Delete all local branchs but current branch"
			},
			{
				"command": "extension.addAction",
				"title": "Add an action"
			}
		],
		"configuration": {
			"type": "object",
			"title": "hoverconverter",
			"properties": {
				"hoverMapping": {
					"type": "object",
					"scope": "application",
					"default": {},
					"description": "Custom hover mapping"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"simple-git": "^2.11.0",
		"translate": "^1.1.2"
	}
}
