{
	"name": "wyn-dev-tools",
	"displayName": "Wyn Dashboard Development Tools",
	"description": "1.Add an action.2.Delete all branchs but current branch.3.Delete",
	"version": "0.0.17",
	"publisher": "boolxiao",
	"repository": "https://github.com/woaiiimipi/wyn-dev-tools",
	"homepage": "https://github.com/woaiiimipi/wyn-dev-tools",
	"icon": "wyn.png",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"keywords": [
		"wyn",
		"dashboard",
		"dev",
		"wyn-dev-tools",
		"wyn dev tools"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "wyn.deleteAllBranch",
				"title": "wyn: Delete local branch"
			},
			{
				"command": "wyn.addAction",
				"title": "wyn: Add an action"
			},
			{
				"command": "wyn.zhTw",
				"title": "wyn: zh -> zh_TW"
			},
			{
				"command": "wyn.enZhZhTw",
				"title": "wyn: en -> zh"
			},
			{
				"command": "wyn.pushToOrigin",
				"title": "bool: Push to origin"
			},
			{
				"command": "wyn.importScssToMain",
				"title": "wyn: Import current file to closest main.scss."
			},
			{
				"command": "wyn.toRender",
				"title": "wyn: Go to render"
			}
		],
		"configuration": {
			"type": "object",
			"title": "wyn-dev-tools",
			"properties": {
				"hoverMapping": {
					"type": "object",
					"scope": "application",
					"default": {},
					"description": "Custom hover mapping"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@typescript-eslint/typescript-estree": "^3.7.0",
		"node-opencc": "^2.0.1",
		"simple-git": "^2.11.0",
		"translate": "^1.1.2"
	}
}
